{
  "extends": [
    "config:base"
  ],
  "commitMessagePrefix": ":arrow_up: ",
  "regexManagers": [
    {
      "fileMatch": ["^Casks/[^/]+[.]rb$"],
      "matchStrings": ["version \"(?<currentValue>.*?)\"\n"],
      "datasourceTemplate": "github-releases",
      "depNameTemplate": "Dygmalab/Bazecor",
      "extractVersionTemplate": "^v(?<version>.*)$"
    }
  ]
}
